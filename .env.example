# Application hostname
# APP_HOSTNAME="localhost"

# Application port
# APP_PORT=8000

# Maximum size of the cache in bytes
# CACHE_SIZE_LIMIT=1073741824

# Number of seconds the entries are kept in cached
# CACHED_TTL_SECONDS=604800

# Enable private datasets
# DATASETS_ENABLE_PRIVATE=False

# Number of rows extracted from the dataset, if not specified otherwise
# EXTRACT_ROWS_LIMIT=100

# Hugging Face token (see https://huggingface.co/settings/token)
# HF_TOKEN=

# Log level
# LOG_LEVEL = "INFO"

# Number of uvicorn workers
# WEB_CONCURRENCY = 1
